{
  "name": "@zkshuffle/contracts",
  "version": "0.0.9",
  "main": "index.js",
  "license": "MIT",
  "description": "Out-of-box smart contracts library enabling DApps to easily integrate ZK stacks",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@typechain/ethers-v5": "^10.1.1",
    "@typechain/hardhat": "^6.1.4",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.5",
    "hardhat-contract-sizer": "^2.6.1",
    "solhint": "^3.3.7",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomiclabs/hardhat-solpp": "^2.0.1",
    "@semaphore-protocol/contracts": "^2.6.1",
    "@zkshuffle/circuits": "0.0.9",
    "@zkshuffle/jssdk": "0.0.9",
    "@zkshuffle/proof": "0.0.9",
    "circomlibjs": "^0.1.7",
    "node-pandoc": "^0.3.0",
    "snarkjs": "^0.7.0",
    "solidity-coverage": "^0.8.2",
    "solidity-docgen": "^0.6.0-beta.32"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "compile": "hardhat compile --verbose",
    "lint:check": "solhint --config ./.solhint.json  \"contracts/**/*.sol\"",
    "lint:fix": "solhint --config ./.solhint.json  \"contracts/**/*.sol\" --fix",
    "test": "hardhat test tests/*.ts",
    "pub2npmjs": "npm publish --access=public",
    "deploy": "hardhat deploy",
    "contract-size": "hardhat size-contracts",
    "doc": "hardhat docgen"
  },
  "files": [
    "src",
    "dist",
    "types",
    "artifacts",
    "artifacts/cache",
    "types/artifacts/cache",
    "types/factories/artifacts/cache",
    "dist/types/factories/artifacts/cache",
    "dist/types/artifacts/cache",
    "contracts",
    "hardhat.config.js",
    "helper",
    "convert.js",
    "adocs"
  ]
}
